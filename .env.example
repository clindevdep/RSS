# RSS Processor Environment Variables
# Copy this file to .env and fill in your actual values

# Inoreader Authentication (required for automatic login)
INOREADER_EMAIL=your-email@example.com
INOREADER_PASSWORD=your-password

# Database Configuration (optional, defaults to SQLite)
# DATABASE_URL=postgresql://user:password@localhost/rss_processor

# Obsidian Integration
# OBSIDIAN_VAULT_PATH=/path/to/your/obsidian/vault

# Processing Configuration (optional)
# MAX_ARTICLES_PER_DAY=100
# SUMMARY_MAX_LENGTH=150
# SURPRISE_ARTICLES_COUNT=5

# Logging (optional)
# LOG_LEVEL=INFO
# LOG_FILE=./logs/rss_processor.log